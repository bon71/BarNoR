{
  "$schema": "https://storage.googleapis.com/anthropic-static-files/schemas/claude-code/settings.1.0.0.json",
  "permissions": {
    "deny": [
      "Read(./.env)",
      "Read(./.env.*)",
      "Read(./secrets/**)",
      "Read(./config/credentials.json)",
      "Read(~/.aws/credentials)",
      "Read(~/.ssh/*)"
    ]
  },
  "shellIntegration": {
    "enabled": true,
    "sourceFiles": [
      "~/.zshenv",
      "~/.zshrc"
    ]
  }
}
